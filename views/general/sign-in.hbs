<body>
  {{>header}}


<div class="login">

    <br>
    <fieldset>
    <h1 class="sign-in-text">Welcome Back</h1> <br>

    <label class="email-text" for="email">Email Address</label>
    <input type="email" id="email" name="email" required placeholder="Email Address"> <br>

    <label class="password-text" for="password">Password</label>
    <input type="password" id="password" name="password"  placeholder="Password" > <br> <br>

    <input type="submit" value="Login" class="button">
      
  </fieldset>

</div>

  {{>footer}}
</body>